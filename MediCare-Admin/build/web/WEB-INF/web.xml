<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>AdminHomeController</servlet-name>
        <servlet-class>Controllers.AdminHomeController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminHomeController</servlet-name>
        <servlet-class>AdminHomeController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminDoctorController</servlet-name>
        <servlet-class>Controllers.AdminDoctorController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddDoctorController</servlet-name>
        <servlet-class>Controllers.AddDoctorController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditDoctorController</servlet-name>
        <servlet-class>Controllers.EditDoctorController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteDoctorController</servlet-name>
        <servlet-class>Controllers.DeleteDoctorController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminHomeController</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminDoctorController</servlet-name>
        <url-pattern>/doctor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddDoctorController</servlet-name>
        <url-pattern>/adddoctor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditDoctorController</servlet-name>
        <url-pattern>/editdoctor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteDoctorController</servlet-name>
        <url-pattern>/deletedoctor</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
